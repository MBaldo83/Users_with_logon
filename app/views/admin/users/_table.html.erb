<!-- admin users table -->
<table>
  <tr>
    <th>Name </th>
    <th>Email </th>
    <th>Admin </th>
  </tr>
<% @users.each do |user| %>
    
	<td><%= user.name%></td>
    <td><%= user.email %></td>
	<% if user.admin %> <td> yes </td> 
		<% else %> <td> no </td>
	<% end %>
	<td><%= link_to 'Show', user %></td>
	<td><%= link_to "Edit", edit_admin_user_path(user) %></td>
   	<td><%= link_to "Delete", user, :method => :delete, :confirm => 'Are you sure?',  
									 :title => "Delete User" %></td>
  </tr>
<% end %>
</table>