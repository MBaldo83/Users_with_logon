<header>
 <% logo = image_tag("logo_transparent.png", :alt => "Ten Events", :class => "round") %>
  <%= link_to logo, root_path %>
   <nav class="round">
    <ul>
	<% if !signed_in? %>
	<li><%= link_to "Log in", signin_path %></li>
	<li><%= link_to "New Member", new_user_path %></li>
	<% else %>
	
   		<% if !admin_user? %>
   		  <li><%= link_to "Summary", root_path %></li>
   		  <li><%= link_to "Users", users_path %></li>
   		  <% if signed_in? %>
   		  <li><%= link_to "Sign out", signout_path, :method => :delete %></li>
   		  <% end %>
   		<% end %>
   		
   		<% if admin_user? %>
   		  <li><%= link_to "Admin Summary", admin_path %></li>
   		  <li><%= link_to "Users", admin_users_path %></li>
   		  <% if signed_in? %>
   		  <li><%= link_to "Sign out", signout_path, :method => :delete %></li>
   		  <% end %>
   		<% end %>
	<% end %>
	
    </ul>
  </nav>
</header>

