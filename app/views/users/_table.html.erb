<table>
  <tr>
    <th>Name </th>
    <th>Email </th>
    <th>Admin ?</th>
  </tr>
<% @users.each do |user| %>
    
	<td><%= user.name%></td>
    <td><%= user.email %></td>
    <td> Not implemented  </td>
	<td><%= link_to 'Show', user %></td>
	<td><%= link_to "Edit", polymorphic_path(user, :action => :edit) %></td>
   	<td><%= link_to "Delete", user, :method => :delete, :confirm => 'Are you sure?',  
									 :title => "Delete User" %></td>
  </tr>
<% end %>
</table>